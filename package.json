        "build-ts": "tsc --project tsconfig.webdriver-server.json",
        "build": "npm run build-ts && npm run lint",
        "lint": "tsc --project tsconfig.webdriver-server.json --noEmit && eslint \"webdriver-server/**/*.{js,ts}\" --quiet --fix",
        "debug": "nodemon --inspect webdriver-server/dist/src/server.js",
        "serve": "node --insecure-http-parser webdriver-server/dist/src/server.js",
        "start": "npm run serve",
        "start-pm2": "pm2 start .pm2-process.json",
        "watch-node": "nodemon --trace-warnings webdriver-server/dist/src/server.js",
        "watch-ts": "tsc --project tsconfig.webdriver-server.json -w",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\""
